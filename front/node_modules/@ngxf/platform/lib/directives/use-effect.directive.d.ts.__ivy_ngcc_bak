import { DoCheck, OnChanges, OnDestroy, SimpleChanges, TemplateRef, ViewContainerRef } from '@angular/core';
export declare class UseEffectDirective implements OnChanges, DoCheck, OnDestroy {
    private templateRef;
    private vcr;
    private context;
    useEffect: Function;
    useEffectOn: [];
    private onDestroyCallback;
    private viewRef;
    constructor(templateRef: TemplateRef<{}>, vcr: ViewContainerRef);
    ngOnChanges(changes: SimpleChanges): void;
    ngDoCheck(): void;
    ngOnDestroy(): void;
    private init;
    private destroy;
}
