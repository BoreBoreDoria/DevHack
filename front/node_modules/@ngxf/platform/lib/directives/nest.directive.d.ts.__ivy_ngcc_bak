import { OnChanges, OnDestroy, SimpleChanges, TemplateRef, ViewContainerRef } from '@angular/core';
interface NestContext {
    $implicit: any;
    nest: any;
}
export declare class NestDirective implements OnChanges, OnDestroy {
    private templateRef;
    private viewContainerRef;
    nest: TemplateRef<NestContext>[];
    nestOf: TemplateRef<NestContext>[];
    private context;
    private viewRef;
    constructor(templateRef: TemplateRef<NestContext>, viewContainerRef: ViewContainerRef);
    ngOnChanges(changes: SimpleChanges): void;
    ngOnDestroy(): void;
    private onTemplatesDidChanged;
    private create;
    private update;
    private destroy;
}
export {};
